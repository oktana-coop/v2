  "version": "0.1.0",
  "type": "module",
  "main": "./dist/main/index.js",
  "packageManager": "pnpm@10.7.1",
  "scripts": {
    "app:dir": "electron-builder --dir",
    "app:dist": "electron-builder",
    "build": "electron-vite build --outDir=dist",
    "compile": "tsc",
    "dev": "electron-vite dev -w --outDir=dist",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 8",
    "postinstall": "electron-builder install-app-deps",
    "prebuild": "pnpm run compile",
    "start": "electron-vite preview --outDir=dist",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "test": "jest ./src --passWithNoTests"
  },
  "build": {
    "appId": "dev.oktana.v2",
    "files": [
      "dist/**/*",
      "package.json"
    ],
    "directories": {
      "output": "bin"
    }
  },
